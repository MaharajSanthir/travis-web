<div class="cron-job-text branch-name">{{cron.branch.name}}</div>
<div class="cron-job-text enqueuing-interval">{{intervalText}}</div>
<div class="cron-job-text disable-by-build">{{disableByBuild}}</div>
<div class="cron-job-value">{{pretty-date cron.next_run}}</div>
<div class="cron-job-value">{{pretty-date cron.last_run}}</div>
<div class="cron-job-action">
  <div class="tooltip">
    {{#if delete.isRunning}}
      {{loading-indicator}}
    {{else}}
      <a href="#" title="" {{action (perform delete) on="click"}}>
        <span class="icon-delete"></span>
      </a>
      <div class="tooltip-bubble">Delete</div>
    {{/if}}
  </div>
</div>
